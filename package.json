{
  "name": "ppl-site",
  "sideEffects": false,
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "runEastStg": "ng serve --configuration=eastStg",
    "runEastPrd": "ng serve --configuration=eastPrd",
    "runOnlineStg": "ng serve --configuration=onlineStg",
    "runOnlinePrd": "ng serve --configuration=onlinePrd",
    "runWestStg": "ng serve --configuration=westStg",
    "runWestPrd": "ng serve --configuration=westPrd",
    "runAusStg": "ng serve --configuration=ausStg",
    "runAusPrd": "ng serve --configuration=ausPrd",
    "deployToGithub": "ng deploy --base-href=/ppl-webapp/",
    "build-east": "ng build --output-path docs/east/Prd --base-href /east/ --configuration=eastPrd",
    "build-west": "ng build --base-href /west/ --configuration=westStg",
    "build-aus": "ng build --base-href /aus/ --configuration=ausPrd",
    "build-online": "ng build --base-href /online/ --configuration=onlinePrd",
    "build-unplugged": "ng build --base-href /unplugged/ --configuration=unpluggedPrd"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/architect": "^0.1702.2",
    "@angular/animations": "^16.1.3",
    "@angular/cdk": "^16.1.3",
    "@angular/common": "^16.1.3",
    "@angular/compiler": "^16.1.3",
    "@angular/core": "^16.1.3",
    "@angular/forms": "^16.1.3",
    "@angular/material": "^16.1.3",
    "@angular/platform-browser": "^16.1.3",
    "@angular/platform-browser-dynamic": "^16.1.3",
    "@angular/router": "^16.1.3",
    "@zxing/browser": "^0.1.4",
    "@zxing/library": "^0.20.0",
    "angular-in-memory-web-api": "^0.16.0",
    "angularx-qrcode": "^17.0.0",
    "canvas-confetti": "^1.6.0",
    "ngx-cookie-service": "^16.0.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.0",
    "zlib": "^1.0.5",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.3",
    "@angular/cli": "^16.1.3",
    "@angular/compiler-cli": "^16.1.3",
    "@types/jasmine": "^4.3.5",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^20.4.0",
    "angular-cli-ghpages": "^1.0.6",
    "codelyzer": "^6.0.2",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jasmine-core": "^5.0.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "prettier": "^2.8.8",
    "protractor": "~7.0.0",
    "sass": "^1.63.6",
    "schematics-scss-migrate": "^2.3.17",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
